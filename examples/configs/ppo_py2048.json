{
    "type": "ppo_agent",

    "update_mode": {
        "unit": "episodes",
        "batch_size": 10,
        "frequency": 10
    },
    "memory": {
        "type": "latest",
        "include_next_states": false,
        "capacity": 5000
    },

    "actions_exploration": {
        "type": "epsilon_decay",
        "initial_epsilon": 1.0,
        "final_epsilon": 0.05,
        "timesteps": 2000000
    },

    "step_optimizer": {
        "type": "adam",
        "learning_rate": 1e-1
    },
    "subsampling_fraction": 0.1,
    "optimization_steps": 50,

    "discount": 0.99,
    "entropy_regularization": 0.01,
    "gae_lambda": null,
    "likelihood_ratio_clipping": 0.2,


    "saver": {
        "directory": null,
        "seconds": 600
    },
    "summarizer": {
        "directory": null,
        "labels": [],
        "seconds": 120
    },

  "states_preprocessing": [
      {
          "type": "increase_dimension"
      },
      {
          "type": "normalize"
      }
  ]
}
