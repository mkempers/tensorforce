{
    "type": "dqn_agent",
    "double_q_model": true,
    "memory": {
        "type": "replay",
        "capacity": 10000,
        "include_next_states": true
    },

    "optimizer": {
      "type": "adam",
      "learning_rate": 1e-2
    },

    "discount": 0.97,

    "actions_exploration": {
        "type": "epsilon_decay",
        "initial_epsilon": 1.0,
        "final_epsilon": 0.005,
        "timesteps": 1000000
    },

    "states_preprocessing": [
        {
            "type": "increase_dimension"
        }
    ]
}
